{
  "name": "geoprocessing-seasketch-demo",
  "version": "1.0.0",
  "description": "SeaSketch geoprocessing module for marine spatial analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "vercel dev",
    "dev": "vercel dev",
    "deploy": "vercel --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:vercel": "node test-vercel.js",
    "lint": "eslint 'src/**/*.ts' 'api/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'api/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@seasketch/geoprocessing": "^6.0.0",
    "@turf/area": "^6.5.0",
    "@turf/intersect": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@vercel/node": "^3.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
